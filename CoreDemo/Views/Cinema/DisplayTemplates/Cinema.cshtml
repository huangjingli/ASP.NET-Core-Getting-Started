@model Cinema
    <tr>
        <th scope="row">@Model.Id</th>
        <td><a asp-controller="Movie" asp-action="Index" asp-route-cinemaId="@Model.Id">@Model.Name</a></td>
        <td>@Model.Location</td>
        <td>@Model.Capacity</td>
        <td><a asp-controller="Cinema" asp-action="Edit" asp-route-cinemaId="@Model.Id">编辑</a></td>
        <td><a asp-controller="Cinema" asp-action="Delete" asp-route-cinemaId="@Model.Id">删除</a></td>
    </tr>